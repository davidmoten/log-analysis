<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:configuration xmlns:ns2="http://org.moten.david/log-analysis/configuration">
	<parser>
            <pattern>^(\d\d\d\d-\d\d-\d\d \d\d:\d\d:\d\d\.\d\d\d) +(\S+) +(\S+) +(\S+)? ?- (.*)$</pattern>
            <patternGroups>logTimestamp,logLevel,logLogger,threadName,logMsg</patternGroups>
            <timestampFormat>yyyy-MM-dd HH:mm:ss.SSS</timestampFormat>
            <timezone>UTC</timezone>
            <multiline>false</multiline>
    </parser>
    <group>
        <log>
            <path>/ausdev/container/logs/cts/cts.log.*</path>
            <watch>true</watch>
        </log>
        <log>
            <path>/ausdev/container/logs/cts/cts.log\..*</path>
            <watch>false</watch>
        </log>
        <log>
            <path>/ausdev/container/logs/cts/cts-adapter-ais.log.*</path>
            <watch>true</watch>
        </log>
        <log>
            <path>/ausdev/container/logs/cts/cts-adapter-ais.log\..*</path>
            <watch>false</watch>
        </log>
        <log>
            <path>/ausdev/container/logs/cts/cts-adapter-flight-info-service.log.*</path>
            <watch>true</watch>
        </log>
        <log>
            <path>/ausdev/container/logs/cts/cts-adapter-flight-info-service.log\..*</path>
            <watch>false</watch>
        </log>
    </group>
</ns2:configuration>